<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chenxiaobo.shardingjdbc.dao.OrderDao">

    <insert id="save">
        insert into order
        (
            order_id,
            goods_id,
            create_time
        )
        values
        (
            #{orderId},
            #{goodsId},
            #{createTime}
        )
    </insert>

</mapper>